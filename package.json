{
  "name": "nyt-react",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'nyt-search-react/*'\" \"npm run nyt-search-react\"",
    "nyt-search-react": "cd nyt-search-react && npm run start",
    "install": "cd nyt-search-react && yarn install",
    "build": "cd nyt-search-react && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geochanto/nyt-react.git"
  },
